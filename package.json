{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "load:test": "artillery run load-test/votaciones-test.yml",
        "load:report": "node -e \"const fs=require('fs'); const dir='load-test/reports'; if(!fs.existsSync(dir)) fs.mkdirSync(dir, { recursive: true }); const timestamp=new Date().toISOString().replace(/[:.]/g,'-'); const json=`${dir}/report-${timestamp}.json`; const html=`${dir}/report-${timestamp}.html`; require('child_process').execSync(`npx artillery run --output ${json} load-test/votaciones-test.yml && npx artillery report --output ${html} ${json}`, {stdio:'inherit'});\""
    },
    "devDependencies": {
        "@inertiajs/vue3": "^1.0.0",
        "@tailwindcss/forms": "^0.5.3",
        "@vitejs/plugin-vue": "^5.0.0",
        "artillery": "^2.0.21",
        "autoprefixer": "^10.4.12",
        "axios": "^1.6.4",
        "laravel-vite-plugin": "^1.0.0",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.2.1",
        "vite": "^5.0.0",
        "vue": "^3.4.0"
    },
    "dependencies": {
        "@heroicons/vue": "^2.1.1",
        "@primeuix/themes": "^1.0.0",
        "aos": "^2.3.4",
        "chart.js": "^4.5.0",
        "chartjs-plugin-datalabels": "^2.2.0",
        "date-fns": "^3.6.0",
        "dayjs": "^1.11.13",
        "driver.js": "^1.3.6",
        "face-api.js": "^0.22.2",
        "firebase": "^11.6.1",
        "primevue": "^4.3.2",
        "vue-easy-lightbox": "^1.19.0",
        "vue-qrcode": "^2.2.2",
        "vue-qrcode-component": "^2.1.1",
        "vue-recaptcha-v3": "^2.0.1",
        "vue-sweetalert2": "^5.0.5",
        "vue-toast-notification": "^3.1.2"
    }
}
